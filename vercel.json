{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*\\.(js|css))", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "/public/index.php" }
    ]
  }
